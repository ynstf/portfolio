{
    "version": 2,
    "builds": [
      {
        "src": "myproject/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9",
          "buildCommand": "pip install -r requirements.txt"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "myproject/wsgi.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "myproject.settings",
      "PYTHONUNBUFFERED": "1"
    }
  }
  