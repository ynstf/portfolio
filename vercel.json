{
    "version": 2,
    "builds": [
    {
        "src": "vercel_app/wsgi.py",
        "use": "@vercel/python"
    }
    ],
    "routes": [
    {
        "src": "/(.*)",
        "dest": "vercel_app/wsgi.py"
    }
    ],
    "env": {
    "DJANGO_SETTINGS_MODULE": "vercel_app.settings",
    "PYTHONUNBUFFERED": "1"
    }
}

